{
  "name": "@mem0-community/ollama",
  "version": "0.1.1",
  "description": "Ollama embedder and LLM adapter for mem0-community.",
  "license": "Apache-2.0",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "clean": "rimraf dist",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "lint": "prettier --check src/"
  },
  "dependencies": {
    "@mem0-community/core": "workspace:*",
    "ollama": "^0.5.14"
  },
  "devDependencies": {
    "rimraf": "^5.0.10"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mem0-community/mem0-ts.git",
    "directory": "packages/ollama"
  },
  "homepage": "https://github.com/mem0-community/mem0-ts#readme",
  "bugs": {
    "url": "https://github.com/mem0-community/mem0-ts/issues"
  },
  "author": "mem0-community",
  "keywords": [
    "mem0",
    "ollama",
    "llm",
    "embeddings",
    "local"
  ],
  "publishConfig": {
    "access": "public"
  }
}
