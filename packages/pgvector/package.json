    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "lint": "prettier --check src/"
  },
  "dependencies": {
    "@mem0-community/core": "workspace:*",
    "pg": "^8.11.3"
  },
  "devDependencies": {
    "@types/pg": "^8.11.0",
    "rimraf": "^5.0.10"
  },
  "publishConfig": {
    "access": "public"
  }
}
